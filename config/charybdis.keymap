#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/pointing.h>

#define BASE  0
#define NUM   1
#define FUNC  2
#define EXT   3
#define GAME  4
#define MOUSE 5

&lt {
    tapping-term-ms = <200>;
    flavor = "balanced";
    quick-tap-ms = <150>;
};

&soft_off {
    hold-time-ms = <2000>; // Only turn off it the key is held for 2 seconds or longer.
};

/ {
    behaviors {
        hm: homerow_mods {
            compatible = "zmk,behavior-hold-tap";
            bindings = <&kp>, <&kp>;

            #binding-cells = <2>;
            tapping-term-ms = <200>;
            flavor = "tap-preferred";
            require-prior-idle-ms = <125>;
            quick-tap-ms = <200>;
        };
    };

    combos {
        compatible = "zmk,combos";

        Clear_BLE_Profile {
            bindings = <&bt BT_CLR>;
            key-positions = <0 4 40>;
        };

        space {
            bindings = <&kp SPACE>;
            key-positions = <19 20>;
        };

        enter {
            bindings = <&kp ENTER>;
            key-positions = <16 15>;
        };

        return {
            bindings = <&kp RET>;
            key-positions = <22 23>;
        };

        escape {
            bindings = <&kp ESC>;
            key-positions = <1 0>;
        };

        underline {
            bindings = <&kp UNDER>;
            key-positions = <9 8>;
        };

        dash {
            bindings = <&kp MINUS>;
            key-positions = <7 8>;
        };

        backslsh {
            bindings = <&kp BSLH>;
            key-positions = <34 35>;
        };

        reload {
            bindings = <&kp LC(R)>;
            key-positions = <4 3>;
        };

        undo {
            bindings = <&kp LC(Z)>;
            key-positions = <25 24>;
        };

        cut {
            bindings = <&kp LC(X)>;
            key-positions = <26 25>;
        };

        copy {
            bindings = <&kp LC(C)>;
            key-positions = <27 26>;
        };

        paste {
            bindings = <&kp LC(V)>;
            key-positions = <28 27>;
        };

        select_all {
            bindings = <&kp LC(A)>;
            key-positions = <13 12>;
        };

        save {
            bindings = <&kp LC(S)>;
            key-positions = <14 13>;
        };

        all_caps {
            bindings = <&kp CAPS>;
            key-positions = <15 20>;
        };

        backspace {
            bindings = <&kp BACKSPACE>;
            key-positions = <10 11>;
        };
    };

    macros {
        email: email {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&none &kp B &kp R &kp Y &kp A &kp N &kp AT &kp P &kp E &kp C &kp J &kp O &kp DOT &kp C &kp O &kp M>;
            wait-ms = <0>;
        };

        git_status: git_status {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp S &kp T &kp A &kp T &kp U &kp S>;
            wait-ms = <0>;
        };

        git_pull: git_pull {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp P &kp U &kp L &kp L>;
            wait-ms = <0>;
        };

        git_pull_many: git_pull_many {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(R) &kp G &kp I &kp T &kp SPACE &kp MINUS &kp LEFT_SHIFT &kp C &kp SPACE &kp LBRC &kp RBRC &kp SPACE &kp P &kp U &kp L &kp L &kp SQT>;
            wait-ms = <0>;
        };

        git_push: git_push {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp P &kp U &kp S &kp H>;
            wait-ms = <0>;
        };

        git_add: git_add {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp A &kp D &kp D &kp SPACE &kp DOT>;
            wait-ms = <0>;
        };

        git_diff: git_diff {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp D &kp I &kp F &kp F>;
            wait-ms = <0>;
        };

        git_checkout: git_checkout {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp C &kp H &kp E &kp C &kp K &kp O &kp U &kp T &kp SPACE>;
            wait-ms = <0>;
        };

        git_commit: git_commit {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp G &kp I &kp T &kp SPACE &kp C &kp O &kp M &kp M &kp I &kp T &kp SPACE &kp MINUS &kp M &kp SPACE &kp DOUBLE_QUOTES &kp DOUBLE_QUOTES &kp LEFT>;
            wait-ms = <0>;
        };

        start_dealsuite: start_dealsuite {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings =
                <&kp LC(R)>,
                <&macro_wait_time 50>,
                <&kp C &kp E &kp L &kp E &kp RET>,
                <&macro_wait_time 2000>,
                <&kp LC(LS(N5)) &kp LC(R)>,
                <&macro_wait_time 50>,
                <&kp R &kp U &kp N &kp SPACE &kp D &kp RET>,
                <&macro_wait_time 2000>,
                <&kp LC(LS(N5)) &kp LC(R)>,
                <&macro_wait_time 50>,
                <&kp A &kp P &kp P &kp DOT &kp P &kp RET>,
                <&macro_wait_time 2000>,
                <&kp LC(LS(N5)) &kp LC(R)>,
                <&macro_wait_time 50>,
                <&kp R &kp U &kp N &kp S &kp E &kp RET>,
                <&macro_wait_time 2000>,
                <&kp LC(LS(N1)) &kp LC(R)>,
                <&macro_wait_time 50>,
                <&kp S &kp T &kp U &kp D &kp RET>;
        };
    };

